<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LibraryPage">

    <RelativeLayout
        android:id="@+id/rootRL"
        android:layout_width="830dp"
        android:layout_height="1069dp"
        android:background="@drawable/background"
        android:backgroundTintMode="src_over"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="85dp"
        android:shadowColor="#303030"
        android:shadowDx="10"
        android:shadowDy="10"
        android:shadowRadius="12"
        android:text="@string/choose_the_room"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="@string/entry_time"
        android:inputType="time"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5"
        tools:ignore="LabelFor"
        android:background="@drawable/edittext_background"
        android:drawablePadding="19dp"
        android:shadowDx="10"
        android:shadowDy="10"
        android:shadowRadius="12"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#202020"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:text="@string/i_m_in"
        android:onClick="iminbuttonclick2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTime"
        tools:ignore="UsingOnClickInXml"
        android:textStyle="bold"/>

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:autofillHints=""
        android:background="@drawable/edittext_background"
        android:drawablePadding="19dp"
        android:ems="10"
        android:hint="@string/digital_reading_room"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:shadowDx="10"
        android:shadowDy="10"
        android:shadowRadius="12"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#202020"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>